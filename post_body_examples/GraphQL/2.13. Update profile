mutation UpdateProfile($id: ID!, $avatar: String, $sex: String, $birthday: Int, $country: String, $street: String, $city: String, $memberTypeId: ID, $userId: ID) {
    updateProfile (id: $id, data: {avatar: $avatar, sex: $sex, birthday: $birthday, country: $country, street: $street, city: $city, memberTypeId: $memberTypeId, userId: $userId}) {
        id
    }
}
